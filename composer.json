{
  "name": "amercier/vcloud-sdk",
  "description": "21st century PHP SDK for vCloud Director - Object-oriented, hand-crafted, namespaced, PSR-0 compatible",
  "type": "library",
  "keywords": [
    "vmware",
    "vcloud",
    "director",
    "api",
    "sdk",
    "patched"
  ],
  "homepage": "https://github.com/amercier/vcloud-sdk-php",
  "license": "MIT",
  "require": {
    "php": ">=5.3.2"
  },
  "autoload": {
    "psr-0": {
      "VMware\\": "src/"
    }
  },
  "include-path": [
    "src/"
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    }
  ],
  "require-dev": {
    "vmware/vcloud-sdk-patched": "512.0.0",
    "pear-pear/HTTP_Request2": "*",
    "phpunit/phpunit": "3.*"
  }
}
